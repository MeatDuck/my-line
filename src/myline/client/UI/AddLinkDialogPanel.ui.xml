<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:import field='myline.shared.ClientConstants.INSERT_URL_LABEL'/>
	<ui:import field='myline.shared.ClientConstants.OK_LABEL'/>
	<g:VerticalPanel spacing="4">
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="{INSERT_URL_LABEL}"/>
		</g:Cell>
		<g:TextBox styleName="linkenter" ui:field="linkTextBox" focus="true"/>
		<g:Cell horizontalAlignment="ALIGN_RIGHT">
			<g:Button text="{OK_LABEL}" ui:field="okButton"/>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 